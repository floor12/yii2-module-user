{
  "name": "floor12/yii2-module-user",
  "description": "Yii2 module to organize user management without overhead.",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "keywords": [
    "floor12",
    "yii2 module",
    "yii",
    "yii2",
    "yii 2",
    "user"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "floor12",
      "email": "primelifecoder@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.13",
    "yii2mod/yii2-enum": "*",
    "floor12/yii2-phone": "*",
    "floor12/yii2-formhint": "*",
    "floor12/yii2-editmodal": "*",
    "floor12/yii2-module-files": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "floor12\\user\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "floor12\\news\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-editmodal"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-module-files"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-phone"
    }
  ]
}
